  "devDependencies": {
    "@types/express": "^4.17.19",
    "@types/node": "^20.8.4",
    "@types/ramda": "^0.29.6",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2"
